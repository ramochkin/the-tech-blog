<div class="posts">
  {{#each posts}}
  <article class="card">
    <div class="card-header flex-row justify-space-between">
      <h2>
        <a href="/post/{{id}}">{{post_title}}</a>
      </h2>
      <div>
        Created by {{username}}
        Posted on {{format_date createdAt}}
        
      </div>
    </div>

  </article>
  {{/each}}
</div>